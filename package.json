{
  "name": "microfrontends",
  "version": "0.0.1",
  "sideEffects": [
    "*.css",
    "*.less"
  ],
  "scripts": {
    "start": "concurrently \"npm run start:react\" \"npm run start:vue\" \"npm run start:ng\" \"npm run start:microfe\"",
    "start:react": "APP_NAME=react nodemon scripts/index.js --watch scripts/index.js",
    "start:ts": "APP_NAME=reactTS nodemon scripts/index.js --watch scripts/index.js",
    "start:vue": "APP_NAME=vue nodemon scripts/index.js --watch scripts/index.js",
    "start:ng": "APP_NAME=angular nodemon scripts/index.js --watch scripts/index.js",
    "start:microfe": "APP_NAME=microfe nodemon scripts/index.js --watch scripts/index.js",
    "build": "webpack --config scripts/webpack.production.js --progress && npm run clean",
    "build:ts": "APP_NAME=reactTS webpack --config scripts/webpack.production.js --progress && npm run clean:ts",
    "build:vue": "APP_NAME=vue webpack --config scripts/webpack.production.js --progress && npm run clean:vue",
    "build:ng": "APP_NAME=angular webpack --config scripts/webpack.production.js --progress && npm run clean:ng",
    "build:micro": "APP_NAME=microfe webpack --config scripts/webpack.production.js --progress && npm run clean:microfe",
    "clean": "cd app/react && rimraf dist && mv build dist",
    "clean:ts": "cd app/reactTS && rimraf dist && mv build dist",
    "clean:vue": "cd app/vue && rimraf dist && mv build dist",
    "clean:ng": "cd app/angular && rimraf dist && mv build dist",
    "clean:microfe": "cd app/microfe && rimraf dist && mv build dist",
    "server": "nodemon scripts/server.js --watch scripts/server.js",
    "server:ts": "APP_NAME=reactTS nodemon scripts/server.js --watch scripts/server.js",
    "server:vue": "APP_NAME=vue nodemon scripts/server.js --watch scripts/server.js",
    "server:ng": "APP_NAME=angular nodemon scripts/server.js --watch scripts/server.js",
    "server:micro": "APP_NAME=microfe nodemon scripts/server.js --watch scripts/server.js",
    "test": "npm run lint && jest",
    "snap": "jest --updateSnapshot",
    "eslint": "eslint app/**/*.{js,jsx}",
    "lint": "tslint -c tslint.json app/**/*.{ts,tsx,js,jsx}",
    "tslint": "tslint -c tslint.json app/**/*.{ts,tsx}",
    "tslint1": "tslint -c tslint.json -e 'demo/tools/**/*.{ts,tsx}' 'demo/**/*.{ts,tsx}'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ahyiru/notes"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.5.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.2",
    "@vue/babel-preset-jsx": "^1.1.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.0",
    "body-parser": "^1.19.0",
    "colors": "^1.3.3",
    "compression": "^1.7.4",
    "concurrently": "^4.1.2",
    "cors": "^2.8.5",
    "css-loader": "^3.2.0",
    "eslint": "^6.2.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "express": "^4.17.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.19.1",
    "husky": "^3.0.4",
    "jest": "^24.9.0",
    "less": "^3.10.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "morgan": "^1.9.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "react-test-renderer": "^16.9.0",
    "rimraf": "^3.0.0",
    "style-loader": "^1.0.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.1.0",
    "vue-loader": "^15.7.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.1",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "dependencies": {
    "@ivu/router": "0.0.10",
    "@ivu/utils": "0.0.6",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "core-js": "^3.2.1",
    "enzyme": "^3.10.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "vue": "^2.6.10"
  },
  "browserslist": [
    "defaults",
    "not ie < 11",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ]
}
